      Put_Line<span class="br0">(</span><span class="st0">"yes"</span><span class="br0">)</span>;
<span class="kw1">else</span>
      Put_Line<span class="br0">(</span><span class="st0">"no"</span><span class="br0">)</span>;
<span class="kw1">end</span> <span class="kw1">if</span>;
</pre></div>
