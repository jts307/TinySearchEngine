<pre class="de1">
<span class="kw1">if</span> <span class="br0">[</span> <span class="re1">$a</span> <span class="re5">-gt</span> <span class="nu0">0</span> <span class="br0">]</span>; <span class="kw1">then</span>
      <span class="kw3">echo</span> <span class="st0">"yes"</span>
<span class="kw1">else</span>
      <span class="kw3">echo</span> <span class="st0">"no"</span>
