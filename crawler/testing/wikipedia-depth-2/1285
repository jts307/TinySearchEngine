<span class="kw1">if</span> a &gt; <span class="nu0">0</span> <span class="kw1">then</span>
<span class="kw1">begin</span>
      <span class="kw3">writeln</span><span class="br0">(</span>"yes"<span class="br0">)</span>
<span class="kw1">end</span> <span class="kw1">else</span>
<span class="kw1">begin</span>
