 <span class="sy0">*</span>D<span class="sy0">;</span> N<span class="sy0">-</span><span class="nu19">1E4</span><span class="sy0">&amp;&amp;</span> XDrawLine<span class="br0">(</span>e <span class="sy0">,</span>z<span class="sy0">,</span>k<span class="sy0">,</span>N <span class="sy0">,</span>U<span class="sy0">,</span>q<span class="sy0">,</span>C<span class="br0">)</span><span class="sy0">;</span> N<span class="sy0">=</span>q<span class="sy0">;</span> U<span class="sy0">=</span>C<span class="sy0">;</span> <span class="br0">}</span> <span class="sy0">++</span>p<span class="sy0">;</span> <span class="br0">}</span> L<span class="sy0">+=</span>_<span class="sy0">*</span> <span class="br0">(</span>X<span class="sy0">*</span>t <span class="sy0">+</span>P<span class="sy0">*</span>M<span class="sy0">+</span>m<span class="sy0">*</span>l<span class="br0">)</span><span class="sy0">;</span> T<span class="sy0">=</span>X<span class="sy0">*</span>X<span class="sy0">+</span> l<span class="sy0">*</span>l<span class="sy0">+</span>M <span class="sy0">*</span>M<span class="sy0">;</span>
  XDrawString<span class="br0">(</span>e<span class="sy0">,</span>z<span class="sy0">,</span>k <span class="sy0">,</span><span class="nu0">20</span><span class="sy0">,</span><span class="nu0">380</span><span class="sy0">,</span>f<span class="sy0">,</span><span class="nu0">17</span><span class="br0">)</span><span class="sy0">;</span> D<span class="sy0">=</span>v<span class="sy0">/</span>l<span class="sy0">*</span><span class="nu0">15</span><span class="sy0">;</span> i<span class="sy0">+=</span><span class="br0">(</span>B <span class="sy0">*</span>l<span class="sy0">-</span>M<span class="sy0">*</span>r <span class="sy0">-</span>X<span class="sy0">*</span>Z<span class="br0">)</span><span class="sy0">*</span>_<span class="sy0">;</span> <span class="kw1">for</span><span class="br0">(</span><span class="sy0">;</span> XPending<span class="br0">(</span>e<span class="br0">)</span><span class="sy0">;</span> u <span class="sy0">*=</span>CS<span class="sy0">!=</span>N<span class="br0">)</span><span class="br0">{</span>
                                   XEvent z<span class="sy0">;</span> XNextEvent<span class="br0">(</span>e <span class="sy0">,&amp;</span>z<span class="br0">)</span><span class="sy0">;</span>
                                       <span class="sy0">++*</span><span class="br0">(</span><span class="br0">(</span>N<span class="sy0">=</span>XLookupKeysym
