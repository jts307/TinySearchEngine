       <span class="nu0">43</span><span class="sy0">,</span><span class="nu0">119</span><span class="sy0">,</span><span class="nu0">224</span><span class="sy0">,</span> <span class="nu0">71</span><span class="sy0">,</span><span class="nu0">122</span><span class="sy0">,</span><span class="nu0">142</span><span class="sy0">,</span> <span class="nu0">42</span><span class="sy0">,</span><span class="nu0">160</span><span class="sy0">,</span><span class="nu0">104</span><span class="sy0">,</span> <span class="nu0">48</span><span class="sy0">,</span><span class="nu0">247</span><span class="sy0">,</span><span class="nu0">103</span><span class="sy0">,</span> <span class="nu0">15</span><span class="sy0">,</span> <span class="nu0">11</span><span class="sy0">,</span><span class="nu0">138</span><span class="sy0">,</span><span class="nu0">239</span>  <span class="co1">// 16</span>
